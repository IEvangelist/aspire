{
  "name": "aspire.dev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:host": "astro dev --host",
    "start": "astro dev",
    "start:host": "astro dev --host",
    "build": "npm run update:all && astro build",
    "build:production": "npm run update:all && astro build --mode production",
    "preview": "astro preview",
    "preview:host": "astro preview --host",
    "astro": "astro",
    "update:all": "npm run update:integrations && npm run update:github-stats",
    "update:integrations": "node ./scripts/update-integrations.js",
    "update:github-stats": "node ./scripts/update-github-stats.js",
    "update:posts": "node ./scripts/update-posts.js"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.6",
    "@astro-community/astro-embed-bluesky": "^0.1.3",
    "@astrojs/starlight": "^0.35.2",
    "@catppuccin/starlight": "^1.0.2",
    "@fontsource-variable/outfit": "^5.2.6",
    "@fontsource-variable/rubik": "^5.2.6",
    "asciinema-player": "^3.10.0",
    "astro": "^5.13.3",
    "astro-mermaid": "^1.0.4",
    "mermaid": "^11.10.1",
    "sharp": "^0.34.3",
    "starlight-contributor-list": "^0.3.0",
    "starlight-giscus": "^0.6.1",
    "starlight-github-alerts": "^0.1.0",
    "starlight-image-zoom": "^0.13.0",
    "starlight-kbd": "^0.2.0",
    "starlight-links-validator": "^0.17.1",
    "starlight-llms-txt": "^0.6.0",
    "starlight-scroll-to-top": "^0.1.1",
    "starlight-sidebar-topics": "^0.6.0"
  },
  "devDependencies": {
    "astro-embed": "^0.9.0",
    "astro-vtbot": "^2.1.7",
    "node-fetch": "^3.3.2"
  }
}
